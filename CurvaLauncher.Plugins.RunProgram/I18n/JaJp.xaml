<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="StrPluginName">プログラムを実行する</sys:String>
    <sys:String x:Key="StrPluginDescription">PATH 内および登録済みのアプリケーションを実行します</sys:String>

    <sys:String x:Key="StrIncludeDirectories">ディレクトリを含める</sys:String>
    <sys:String x:Key="StrExcludeDirectories">ディレクトリを除外する</sys:String>
    <sys:String x:Key="StrEnableAppsIndexing">アプリケーションの索引を有効にする</sys:String>
</ResourceDictionary>